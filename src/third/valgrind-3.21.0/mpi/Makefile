	$(AM_V_at)$(am__mv) $(DEPDIR)/libmpiwrap_amd64_linux_so-libmpiwrap.Tpo $(DEPDIR)/libmpiwrap_amd64_linux_so-libmpiwrap.Po
#	$(AM_V_CC)source='libmpiwrap.c' object='libmpiwrap_amd64_linux_so-libmpiwrap.o' libtool=no \
